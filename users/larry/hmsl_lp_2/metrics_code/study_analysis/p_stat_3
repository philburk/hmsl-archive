\ note and interval histogram routinesanew task-p_stat_3: COUNT.NOTES { | curr-section curr-group c-dim curr-note -- }		many: whole-piece 0 DO			i at: whole-piece -> curr-section			i -> c-dim			curr-section many: [] 0 DO				i curr-section at: [] -> curr-group				curr-group many: [] 0 DO						i 1 ed.at: curr-group						12 mod -> curr-note						curr-note c-dim p-notes ed.at: []						1+ 						curr-note c-dim p-notes ed.to: []				LOOP			LOOP		LOOP		;: COUNT.INTERVALS { | curr-section curr-group c-dim curr-int -- }		many: whole-piece 0 DO			i at: whole-piece -> curr-section			i -> c-dim			curr-section many: [] 0 DO				i curr-section at: [] -> curr-group				curr-group many: [] 1 DO						i 1 ed.at: curr-group						i 1- 1 ed.at: curr-group						- abs 						12 mod						-> curr-int						curr-int c-dim p-intervals ed.at: []						1+ 						curr-int c-dim p-intervals ed.to: []				LOOP			LOOP		LOOP		;: COUNT.TRITONE-INTERVALS { | curr-section curr-group c-dim curr-int -- }		many: whole-piece 0 DO			i at: whole-piece -> curr-section			i -> c-dim			curr-section many: [] 0 DO				i curr-section at: [] -> curr-group				curr-group many: [] 1 DO					i 1 ed.at: curr-group					i 1- 1 ed.at: curr-group					- abs 					12 mod					dup 5 >					IF						12 swap -					THEN					dup . 					-> curr-int					curr-int c-dim p-tritone-intervals ed.at: []					1+ 					curr-int c-dim p-tritone-intervals ed.to: []				LOOP			LOOP		LOOP		;