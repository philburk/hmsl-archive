\ statistical measures on pitch intervals\ piano study in mixed accentsanew task-pstat_2fvariable t-lmeanfvariable t-maxfvariable t-cmeanfvariable t-cmaxfvariable t-lsdfvariable t-csd: P.GROUP.INTERVALS { | curr-group curr-section -- } 	cr	." Statistics on intervals in each group " cr	." L-Mean, c-mean max, min, range, sd;  max, min, range, sd " cr		cr	many: whole-piece  0	DO		cr i ." Section # : " . cr		i at: whole-piece -> curr-section		curr-section many: [] 0 		DO			i curr-section at: []			-> curr-group			1 curr-group calc.extended.stats: []			1 curr-group mean.linear.int: [] f. tab			1 curr-group mean.combinatorial.int: [] f. tab			1 curr-group max.linear.int: [] dup . tab			1 curr-group min.linear.int: [] dup . tab			- abs . tab 			1 curr-group get.dim.sd': []  f. tab			1 curr-group max.combinatorial.int: [] dup . tab			1 curr-group min.combinatorial.int: [] dup . tab			 - abs . tab			1 curr-group get.dim.comb-sd': []  f. tab			cr		LOOP		cr	LOOP;: P.SECTION.INTERVALS { | curr-group curr-section lmax cmax lmin cmin -- } 	cr	." Statistics on intervals in section " cr	." L-Mean,       lmax, lmin,  range     lsd;        C-Mean, cmax cmin range    csd " cr		cr 	many: whole-piece 0	DO		0.0 t-lmean f!		0.0 t-cmean f!		0.0 t-lsd f!		0.0 t-csd f!		0 -> cmax		0 -> lmax		100000  -> lmin		100000 -> cmin		i at: whole-piece -> curr-section		curr-section many: [] 0 		DO			i curr-section at: []			-> curr-group			1 curr-group calc.extended.stats: []			1 curr-group mean.linear.int: [] t-lmean f@ f+ t-lmean f! 			1 curr-group max.linear.int: [] dup				lmax >				IF					-> lmax				ELSE					drop				THEN			1 curr-group min.linear.int: [] dup				lmin <				IF					-> lmin				ELSE					drop				THEN			1 curr-group get.dim.sd': []  t-lsd f@ f+ t-lsd f! 			1 curr-group mean.combinatorial.int: [] t-cmean f@ f+ t-cmean f! 			1 curr-group max.combinatorial.int: [] dup						cmax >				IF					-> cmax				ELSE					drop				THEN			1 curr-group min.combinatorial.int: [] dup						cmin <				IF					-> cmin				ELSE					drop				THEN			1 curr-group get.dim.comb-sd': []  t-csd f@ f+ t-csd f! 		LOOP		t-lmean f@ curr-section many: [] float f/ f. tab		lmax . tab		lmin . tab		lmax lmin - . tab		t-lsd f@ curr-section many: [] float f/ f. tab		t-cmean f@ curr-section many: [] float f/ f. tab		cmax . tab		cmin . tab		cmax cmin - . tab		t-csd f@ curr-section many: [] float f/ f. tab		cr	LOOP;	