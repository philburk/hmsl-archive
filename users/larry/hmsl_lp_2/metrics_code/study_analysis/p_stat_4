\ p_stat_4\ metric analyses\ author lp\ 10/29/93anew task-p_stat_4: SET.UNITY { | curr-group -- }	many: all-groups 0 DO		i at: all-groups -> curr-group		'c mw.unity curr-group put.linear.weight.function: []		'c mw.unity curr-group put.row.weight.function: []		'c mw.unity curr-group put.column.weight.function: []	LOOP;: SET.1/i { | curr-group -- }	many: all-groups 0 DO		i at: all-groups -> curr-group		'c mw.1/i curr-group put.linear.weight.function: []		'c mw.1/i curr-group put.row.weight.function: []		'c mw.1/i curr-group put.column.weight.function: []	LOOP;: P.METRIC.INTERVALS.WHOLE.PIECE.UNITY { | curr-group prev-group -- }	cr ." Metric intervals between adjacent groups, whole piece..."	." Unity weight function " 	cr ." UCD" tab tab ." OCD" tab tab ." UCM" tab tab ." OCM " tab tab ." OLD" tab tab ." OLD " 	cr	set.unity	many: all-groups 1 DO		i . tab		i at: all-groups -> curr-group		i 1- at: all-groups -> prev-group\		curr-group prev-group ucd: [] f. tab		curr-group prev-group ocd: [] f. tab tab \		curr-group prev-group ucm: [] f. tab\		curr-group prev-group ocm: [] f. tab		curr-group prev-group old: [] f. tab		cr	LOOP;: P.METRIC.INTERVALS.WHOLE.PIECE.1/i { | curr-group prev-group -- }	cr ." Metric intervals between adjacent groups, whole piece..."	." 1/n weight function "	cr ." UCD" tab tab ." OCD" tab tab  ." UCM" tab tab ." OCD " tab tab ." OCM" tab tab ." OLD " 	cr	set.1/i	many: all-groups 1 DO		i . tab		i at: all-groups -> curr-group		i 1- at: all-groups -> prev-group\		curr-group prev-group ucd: [] f. tab		curr-group prev-group ocd: [] f. tab tab\		curr-group prev-group ucm: [] f. tab		curr-group prev-group ocm: [] f. tab\		curr-group prev-group old: [] f. tab		cr	LOOP;: P.METRIC.INTERVALS.SECTIONS.UNITY { |  curr-section  curr-group prev-group -- }	cr ." Metric intervals to first group, sections..."	." Unity weight function " 	cr ." UCD" tab tab ." OCD" tab tab ." UCM" tab tab ." OCD " tab tab ." OCM" tab tab ." OLD " 	cr		cr	set.unity	many: whole-piece 0	DO		cr i ." Section # : " . cr 		i at: whole-piece -> curr-section		curr-section many: [] 0 		DO			i . tab			i curr-section at: [] -> curr-group			0 at: curr-section -> prev-group			curr-group prev-group ucd: [] f. tab			curr-group prev-group ocd: [] f. tab			curr-group prev-group ucm: [] f. tab			curr-group prev-group ocm: [] f. tab			curr-group prev-group old: [] f. tab			cr		LOOP	LOOP;: P.METRIC.INTERVALS.SECTIONS.1/i { |  curr-section  curr-group prev-group -- }	cr ." Metric intervals to first group,  sections..."	." Unity weight function " 	cr ." UCD" tab tab ." OCD" tab tab ." UCM" tab tab ." OCD " tab tab ." OCM" tab tab ." OLD " 	cr	set.1/i	many: whole-piece 0	DO		cr i ." Section # : " . cr 		i at: whole-piece -> curr-section		curr-section many: [] 0		DO			i . tab			i curr-section at: [] -> curr-group			0 at: curr-section  -> prev-group			curr-group prev-group ucd: [] f. tab			curr-group prev-group ocd: [] f. tab			curr-group prev-group ucm: [] f. tab			curr-group prev-group ocm: [] f. tab			curr-group prev-group old: [] f. tab			cr		LOOP	LOOP;\ metrics to first and last of each section, ocd, ucd, ocm, ucm: OCD.2d.UNITY { |  curr-section  curr-group first-group last-group -- }	cr ." Metric intervals to first group, sections..."	." Unity weight function " 	cr ." UCD" tab tab ." OCD" tab tab ." UCM" tab tab ." OCD " tab tab ." OCM" tab tab ." OLD " 	cr	set.unity	many: whole-piece 0	DO		cr i ." Section # : " . cr 		i at: whole-piece -> curr-section		curr-section many: [] 0 		DO			i . tab			i curr-section at: [] -> curr-group			0 curr-section at: []  -> first-group			curr-section many: [] 1- curr-section at: [] -> last-group			curr-group first-group ocd: [] f. tab			curr-group last-group ocd: [] f. tabcr		LOOP	LOOP;\ metrics to first and last of each section, ocd, ucd, ocm, ucm: UCD.2d.1/i { |  curr-section  curr-group first-group last-group -- }	cr ." Metric intervals to first group, sections..."	." Unity weight function " 	cr ." UCD" tab tab ." OCD" tab tab ." UCM" tab tab ." OCD " tab tab ." OCM" tab tab ." OLD " 	cr	set.unity	many: whole-piece 0	DO		cr i ." Section # : " . cr 		i at: whole-piece -> curr-section		curr-section many: [] 0 		DO			i . tab			i curr-section at: [] -> curr-group			0 curr-section at: []  -> first-group			curr-section many: [] 1- curr-section at: [] -> last-group			curr-group first-group ucd: [] f. tab			curr-group last-group ucd: [] f. tabcr		LOOP	LOOP;\ metrics to first and last of each section, ocd, ucd, ocm, ucm: UCM.2d.1/i { |  curr-section  curr-group first-group last-group -- }	cr ." Metric intervals to first group, sections..."	." Unity weight function " 	cr ." UCD" tab tab ." OCD" tab tab ." UCM" tab tab ." OCD " tab tab ." OCM" tab tab ." OLD " 	cr	set.1/i	many: whole-piece 0	DO		cr i ." Section # : " . cr 		i at: whole-piece -> curr-section		curr-section many: [] 0 		DO			i . tab			i curr-section at: [] -> curr-group			0 curr-section at: []  -> first-group			curr-section many: [] 1- curr-section at: [] -> last-group			curr-group first-group ucm: [] f. tab			curr-group last-group ucm: [] f. tabcr		LOOP	LOOP;\ metrics to first and last of each section, ocd, ucd, ocm, ucm: OCM.2d.UNITY { |  curr-section  curr-group first-group last-group -- }	cr ." Metric intervals to first group, sections..."	." Unity weight function " 	cr ." UCD" tab tab ." OCD" tab tab ." UCM" tab tab ." OCD " tab tab ." OCM" tab tab ." OLD " 	cr	set.unity	many: whole-piece 0	DO		cr i ." Section # : " . cr 		i at: whole-piece -> curr-section		curr-section many: [] 0 		DO			i . tab			i curr-section at: [] -> curr-group			0 curr-section at: []  -> first-group			curr-section many: [] 1- curr-section at: [] -> last-group			curr-group first-group ocm: [] f. tab			curr-group last-group ocm: [] f. tabcr		LOOP	LOOP;