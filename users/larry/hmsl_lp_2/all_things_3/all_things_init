\ initialization file for all_thingsanew task-all_things_init: INIT.ALL_THINGS	fpinit	1 weighted? ! \ set to use weighted stats	0 framed? !   \ don't use frames		1 crossfade-type ! \ use exponential crossfade	\ initialize first distribution	init.all_things_objects 	." initialized "	.s	0 at: p-dist-array curr-dist ! ." . " 	0 at: p-dist-array source-dist !	1 at: p-dist-array target-dist !	.s	." set current distribution " cr	fill.adjusted.array ." adjusted "	0 curr-time !	0 curr-number-notes !	init.player;init.all_things