\ 17 forms\ setup 3 listsanew task-forms_listsob.objlist forms-1ob.objlist forms-2ob.objlist forms-3ob.objlist forms-all: INIT.FORMS-LISTS	17 new: forms-1	17 new: forms-2	17 new: forms-3	51 new: forms-all	cr ." Lists initialized " cr;: MAKE.ONE { | shape-add -- }	instantiate ob.m.shape -> shape-add	 17 1 shape-add new: []	 shape-add;	: FILL.FORMS-LISTS	17 0 DO		make.one dup add: forms-1 add: forms-all		make.one dup add: forms-2 add: forms-all		make.one dup add: forms-3 add: forms-all		." . "	LOOP	cr ." Lists filled with shapes " cr;: MAKE.SOURCE.SHAPE { | s-shape -- }	0 at: forms-1 -> s-shape	stuff{	5 60 42 36 73 	61 59 53 42 90 3 95 	5 95 30 40 50 	s-shape }stuff: [];: MAKE.SOURCE.SHAPE { | s-shape -- }	16 at: forms-1 -> s-shape	stuff{	5 60 42 36 73 	61 59 53 42 90 3 95 	5 95 30 40 50 	s-shape }stuff: [];: PRINT.COLUMN { shape-add -- }	shape-add many: [] 0	DO		tab i 0 shape-add ed.at: []		.		cr	LOOP;: DO.FORMS-LISTS	init.forms-lists	fill.forms-lists	make.source.shape	0 0 at: forms-1 ph: []\	print: forms-all;: DO.FORMS	do.forms-lists;: TERM.forms-LISTS	freeall: forms-all	free: forms-all	free: forms-1	free: forms-2	free: forms-3;if.forgotten term.forms-lists