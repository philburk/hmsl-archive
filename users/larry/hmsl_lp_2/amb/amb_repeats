\ jobs and objects for amb\ added: 8/6/97anew task-amb_repeats10 k: amb_repeat_length: INIT.AMB-REPEATS	4 amb_repeat_length new: amb-repeat-shape	4 0 DO		amb_repeat_length 0 		DO			60 		LOOP		add: amb-repeat-shape	LOOP	print: amb-repeat-shape	4 new: amb-repeat-counter	4 0 DO		0 i to: amb-repeat-counter	LOOP;v: add-counter: ADD.TO.REPEAT-shape { note job# -- }	\ old way to do it, a LIFO stack\	amb_repeat_length 1- 0\	DO\		job# amb_repeat_length 2 - i -  \		ed.at: amb-repeat-shape \ get the previous one...\		job# amb_repeat_length  1- i -  \		ed.to: amb-repeat-shape \ put it in the next one\	LOOP\	note job# 0 ed.to: amb-repeat-shape\ new way, just place them in the shape as they come out...	note job# add-counter @ ed.to: amb-repeat-shape	add-counter @ 1+ amb_repeat_length mod add-counter !;		 	: TERM.AMB-REPEATS	free: amb-repeat-shape	free: amb-repeat-counter;if.forgotten term.amb-repeats	