\ examples for soundhack article\ connecticut conference on technology\ 1/18/95anew task-connecticut_examplesob.mutant sawtooth-shapeob.mutant triangle-shapeob.mutant gauss-shapeob.mutant mutant-shapeob.new-shape change-shape-1: CALC drop 1000 choose ;	: TERM.EXAMPLES	free: mutant-shape	free: gauss-shape	free: sawtooth-shape	free: triangle-shape	free: change-shape-1;logto " foobar "	: TEST.EXAMPLE1\	6 1 new: change-shape\	stuff{ 100 20 25 100 80 0 }stuff: change-shape	100 1 new: mutant-shape	100 1 new: sawtooth-shape	100 1 new: gauss-shape	100 1 new: triangle-shape	100 0 DO		1000 i 10 * - add: sawtooth-shape				1000 i i *  10 * - 0 max add: triangle-shape		i 50 < 		IF			i i choose choose choose choose choose choose choose - 			20 *		THEN		i 50 >= 		IF 			100			i i 50 mod choose choose choose choose choose choose choose			-			-			20 * 		THEN		add: gauss-shape		0 add: mutant-shape	LOOP\	'c calc put..function: mutant-shape\	change-shape-1 put..shape: mutant-shape	0 put.min.: mutant-shape	1000 put.max.: mutant-shape	sawtooth-shape put.source: mutant-shape	gauss-shape put.target: mutant-shape	use.fundamental: mutant-shape	0 0 put.fundamental: mutant-shape	use.fundamental: triangle-shape	0 0 put.fundamental: triangle-shape	use.fundamental: sawtooth-shape	0 0 put.fundamental: sawtooth-shape	." mutant-shape " 0 ph: mutant-shape cr\	." triangle " 0 ph: triangle-shape cr 	." sawtooth " 0 ph: sawtooth-shape cr	10 1 DO		cr cr ." MUTATION " i . 		100 i * put.: mutant-shape		lcm/iuim: mutant-shape		0 ph: mutant-shape	LOOP	." gauss " 0 ph: gauss-shape cr	interpolate.-shape: mutant-shape;: SETUP.EXAMPLE2	free: mutant-shape	free: sawtooth-shape	free: gauss-shape	free: triangle-shape	100 1 new: mutant-shape	100 1 new: sawtooth-shape	100 1 new: gauss-shape	100 1 new: triangle-shape	100 0 DO		1000 i 10 * - add: sawtooth-shape				1000 i i *  10 * - 0 max add: triangle-shape		i 50 < 		IF			i i choose choose choose choose choose - 			20 *		THEN		i 50 >= 		IF 			100			i i 50 mod choose choose choose choose choose			-			-			20 * 		THEN		add: gauss-shape		0 add: mutant-shape	LOOP\	'c calc put..function: mutant-shape\	change-shape-1 put..shape: mutant-shape	0 put.min.: mutant-shape	1000 put.max.: mutant-shape	sawtooth-shape put.source: mutant-shape	gauss-shape put.target: mutant-shape	use.fundamental: mutant-shape	500 0 put.fundamental: mutant-shape	use.fundamental: triangle-shape	500 0 put.fundamental: triangle-shape	use.fundamental: sawtooth-shape	500 0 put.fundamental: sawtooth-shape	100 0 DO 			i 0 ed.at: mutant-shape . cr	LOOP\	." triangle " 0 ph: triangle-shape cr 	." sawtooth shape " cr		100 0 DO 			99 i -  0 ed.at: sawtooth-shape . cr		LOOP	." gauss shape " cr		100 0 DO 			99 i -  0 ed.at: gauss-shape . cr		LOOP	interpolate.-shape: mutant-shape;10 K: #-mutations: TEST.EXAMPLE2	." all done around fundamental of 500 "	setup.example2	cr cr	." USIM "	#-mutations 1+ 1 DO		cr cr ." MUTATION " i . cr		1000  #-mutations / i * put.: mutant-shape		USIM: mutant-shape		100 0 DO 			99 i - 0 ed.at: mutant-shape . cr		LOOP	LOOP	cr cr	." ISIM "	#-mutations 1+ 1 DO		cr cr ." MUTATION " i . cr		1000  #-mutations / i * put.: mutant-shape		ISIM: mutant-shape		100 0 DO 			99 i - 0 ed.at: mutant-shape . cr		LOOP	LOOP	cr cr	." UUIM "	#-mutations 1+ 1 DO		cr cr ." MUTATION " i . cr		1000  #-mutations / i * put.: mutant-shape		UUIM: mutant-shape		100 0 DO 			99 i - 0 ed.at: mutant-shape . cr		LOOP	LOOP	cr cr	." IUIM "	#-mutations 1+ 1 DO		cr cr ." MUTATION " i . cr		1000  #-mutations / i * put.: mutant-shape		IUIM: mutant-shape		100 0 DO 			99 i - 0 ed.at: mutant-shape . cr		LOOP	LOOP	cr cr	." LCM "	#-mutations 1+ 1 DO		cr cr ." MUTATION " i . cr		1000  #-mutations / i * put.: mutant-shape		LCM: mutant-shape		100 0 DO 			99 i - 0 ed.at: mutant-shape . cr		LOOP	LOOP	cr cr	." LCM/IUIM "	#-mutations 1+ 1 DO		cr cr ." MUTATION " i . cr		1000  #-mutations / i * put.: mutant-shape		LCM/IUIM: mutant-shape		100 0 DO 			99 i - 0 ed.at: mutant-shape . cr		LOOP	LOOP	cr cr	." LCM/UUIM "	#-mutations 1+ 1 DO		cr cr ." MUTATION " i . cr		1000  #-mutations / i * put.: mutant-shape		LCM/UUIM: mutant-shape		100 0 DO 			99 i - 0 ed.at: mutant-shape . cr		LOOP	LOOP;	test.example2logendif.forgotten term.examples