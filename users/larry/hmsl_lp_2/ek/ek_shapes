\ shapes for eine kleine...anew task-ek_shapes: FILL.SHAPE { curr-shape val -- stuff it with something }	 stuff{ 60 1 75   60 2 75   60 5 75  curr-shape }stuff: [];: INIT.EK-TEST-SHAPES { | curr-shape -- }	#_ek_channels  new: ek-shapes	#_ek_channels 	0 DO		instantiate ob.shape		-> curr-shape		3 3 curr-shape new: []		curr-shape i fill.shape		curr-shape add: ek-shapes		curr-shape i at: ek-collection add: [] \ add to player	LOOP;: SK  ek_pulse ek_skip  60 ;: ST  ek_pulse ek_step 60 ;: FD  ek_pulse ek_fund 60 ;\ this shape is step then skip: FILL.MEL-SHAPE	75 3 new: mel-shape	stuff{ 		fd st sk fd st sk fd st 			ek_pulse 4 * ek_skip  60 \ 1		fd st sk fd st sk fd st sk fd st 	ek_pulse 5 * ek_skip  60 \ 2		fd st sk fd st sk fd 				ek_pulse 5 * ek_step  60 \ 3		fd st sk fd st sk fd st sk fd 		ek_pulse 4 * ek_step  60 \ 4		fd st sk fd st sk fd st sk		    ek_pulse 5 * ek_fund  60 \ 5		fd st sk fd st sk fd st sk fd st sk			ek_pulse  ek_fund  60  ek_pulse 3 * ek_fund 0 \ last note staccato	}stuff: mel-shape\	print: mel-shape;\ this shape is skip then step: FILL.MEL.SKIP-SHAPE	75 3 new: mel.skip-shape	stuff{ 		fd sk st fd sk st fd sk 			ek_pulse 4 * ek_step  60 \ 1		fd sk st fd sk st fd sk st fd sk	ek_pulse 5 * ek_step  60 \ 2		fd sk st fd sk st fd 				ek_pulse 5 * ek_skip  60 \ 3		fd sk st fd sk st fd sk st fd 		ek_pulse 4 * ek_skip   60 \ 4		fd sk st fd sk st fd sk st		    ek_pulse 5 * ek_fund  60 \ 5		fd sk st fd sk st fd sk st fd sk st			ek_pulse  ek_fund  60  ek_pulse 3 * ek_fund 0 \ last note staccato		}stuff: mel.skip-shape\	print: mel.skip-shape;\ 0 loudnesess for the rhythmic part: FILL.SUS-SHAPE	50 3 new: sus-shape	stuff{ 		8 ek_pulse * ek_fund 60 	ek_pulse 4 * ek_fund 0		11 ek_pulse * ek_fund 60	ek_pulse 5 * ek_fund 0			7 ek_pulse * ek_fund 60 	ek_pulse 5 * ek_fund 0					10 ek_pulse * ek_fund 60	ek_pulse 4 * ek_fund  0			9 ek_pulse * ek_fund 60	  	ek_pulse 5 * ek_fund  0 			12 ek_pulse * ek_fund 60	ek_pulse 4 * ek_fund  0	}stuff: sus-shape\	print: sus-shape;\ 0 loudnesess for the rhythmic part: FILL.RHY-SHAPE	50 3 new: rhy-shape	stuff{ 		8 ek_pulse * ek_fund 0	 		ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60		ek_pulse ek_fund 60		\		10 ek_pulse * ek_fund 0		ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60			ek_pulse ek_fund 60	ek_pulse ek_fund 60			\			6 ek_pulse * ek_fund 0 				ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60			ek_pulse ek_fund 60	ek_pulse ek_fund 60			\					9 ek_pulse * ek_fund 0				ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60		ek_pulse ek_fund 60		\		8 ek_pulse * ek_fund 0	 		ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60	ek_pulse ek_fund 60			ek_pulse ek_fund 60 ek_pulse ek_fund 60	 		\		11 ek_pulse * ek_fund 0	 		ek_pulse ek_fund 60	ek_pulse ek_fund 0	ek_pulse ek_fund 0 		ek_pulse ek_fund 0  \ last note played, staccatto		}stuff: rhy-shape\	print: rhy-shape;: INIT.EK-SHAPES	init.ek-test-shapes;: TERM.EK-SHAPES	freeall: ek-shapes	free: ek-shapes	free: mel-shape	free: sus-shape	free: rhy-shape	free: mel.skip-shape;if.forgotten term.ek-shapes		