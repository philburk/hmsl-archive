\ sent one of the channels to a click track\ ek\ 12/19/95\ lpanew task-ek_channel_to_click: FILL.EK-CLICK-SHAPE { | shape-length -- }	12 16 12 14 14 16 	+ + + + + 2/ -> shape-length	shape-length 3 new: ek-click-shape	shape-length 0 DO	\ fill it with "one" notes, use instrument offset for note		ek_pulse 2 * 1 midi-click-loudness @		add: ek-click-shape	LOOP;: INIT.EK-CLICK-PLAYER	1 new: ek-click-player	ek-click-shape add: ek-click-player	ins-midi-1 put.instrument: ek-click-player	16 put.channel: ins-midi-1	midi-click-note @ put.offset: ins-midi-1	midi-click-preset @ put.preset: ins-midi-1;: INIT.EK-CLICK	fill.ek-click-shape	init.ek-click-player;: TERM.EK-CLICK	free: ek-click-shape	free: ek-click-player;if.forgotten term.ek-click