\ tracker screenanew task-tracker_screen: TRACKER-UNCLOG-MIDI.FUNCTION ( value part -- )	2drop midi.unclog;: BUILD.TRACKER-UNCLOG-MIDI	1 1 new: tracker-unclog-midi	500 200 put.wh: tracker-unclog-midi	9 put.text.size: tracker-unclog-midi	stuff{ " unclog " }stuff.text: tracker-unclog-midi	'c tracker-unclog-midi.function put.down.function: tracker-unclog-midi;	: TRACKER-SHORTEST-NOTE.FUNCTION ( value part -- )	 drop shortest-note ! ;: BUILD.TRACKER-SHORTEST-NOTE	1 1 new: TRACKER-shortest-note	250 250 put.wh: TRACKER-shortest-note	100 -1 put.max: TRACKER-shortest-note	0 -1 put.min: TRACKER-shortest-note	5 0 put.value: TRACKER-shortest-note		'c TRACKER-shortest-note.function put.down.function: TRACKER-shortest-note	'c TRACKER-shortest-note.function put.up.function: TRACKER-shortest-note	'c TRACKER-shortest-note.function put.move.function: TRACKER-shortest-note	9 put.text.size: TRACKER-shortest-note	stuff{ " Deglitch" }stuff.text: TRACKER-shortest-note;: TRACKER-START-PIECE.FUNCTION ( value part -- )	value true =	IF		time@ t-start-time !		3500 100 scg.move " Piece running... " gr.text		true piece-running? !	ELSE		0 t-curr-time !		3500 100 scg.move " Piece stopped... " gr.text		false piece-running? !	THEN;: BUILD.TRACKER-START-PIECE	1 1 new: cg-start-piece	500 300 put.wh: cg-start-piece	10 put.text.size: cg-start-piece	stuff{ " START " }stuff.text: cg-start-piece	'c tracker-start-piece.function put.down.function: cg-start-piece; : CG-BEND-FADER.FUNCTION { val part -- }	val midi.bend ;: BUILD.CG-BEND-FADER	0 0 put.min: cg-bend-fader	hex 4000 decimal 0 put.max: cg-bend-fader	hex 2000 decimal 0 put.value: cg-bend-fader	100 2000 put.wh: cg-bend-fader	9 put.text.size: cg-bend-fader	" bend " put.title: cg-bend-fader	'c cg-bend-fader.function put.down.function: cg-bend-fader	'c cg-bend-fader.function put.move.function: cg-bend-fader; : CG-MOD-FADER.FUNCTION { val part -- }	val 1 midi.control ;: BUILD.CG-MOD-FADER	0 0 put.min: cg-mod-fader	128 0 put.max: cg-mod-fader	0 0 put.value: cg-mod-fader	100 2000 put.wh: cg-mod-fader	9 put.text.size: cg-mod-fader	" mod " put.title: cg-mod-fader	'c CG-MOD-FADER.FUNCTION put.down.function: cg-mod-fader	'c CG-MOD-FADER.FUNCTION put.move.function: cg-mod-fader; : CG-REPEAT-DELAY-FUNCTION { val part -- }	val tracker-random-repeat? ! ;: BUILD.CG-REPEAT-DELAY	1 1 new: cg-repeat-delay	650 300 put.wh: cg-repeat-delay	9 put.text.size: cg-repeat-delay	stuff{ " repeat delay " }stuff.text: cg-repeat-delay	'c CG-REPEAT-DELAY-FUNCTION put.down.function: cg-repeat-delay;	: BUILD.TRACKER.CONTROLS	build.TRACKER-shortest-note	build.cg-tracker-record	build.cg-tracker-play	build.cg-play-params	build.cg-tracker-job	build.cg-tracker-job-params	build.cg-tracker-flammer-params	build.cg-flam-on	build.cg-blaster	build.tracker-unclog-midi	build.tracker-start-piece	build.cg-bend-fader	build.cg-mod-fader	build.cg-fbo1	build.cg-drum-follow	build.cg-fbo1-params	build.cg-drum-follow-params	build.cg-pedal-function	build.cg-reverb	build.cg-repeat-delay\ setup screen	0 scg.selnt	tracker-screen default-screen !	" Always cut off the baseline... " Put.title: TRACKER-screen	41 3 new: TRACKER-screen	cg-tracker-record 200 400 add: tracker-screen	cg-tracker-play 200 2600 add: tracker-screen	cg-repeat-delay 600 2100 add: tracker-screen	cg-play-params 2000 4300 add: tracker-screen	cg-tracker-job 200 950 add: tracker-screen	cg-tracker-job-params 2000 1250 add: tracker-screen	cg-tracker-flammer-params 2900 1200 add: tracker-screen	cg-fbo1-params 4500 1200 add: tracker-screen	cg-drum-follow-params 4700 4400 add: tracker-screen	cg-fbo1 4500 3000 add: tracker-screen	cg-drum-follow 4500 3800 add: tracker-screen	cg-flam-on 200 1500 add: tracker-screen	cg-blaster 200 2100 add: tracker-screen	cg-start-piece 3500 250 add: TRACKER-screen	cg-bend-fader 3500 1000 add: tracker-screen	cg-mod-fader 3800 1000 add: tracker-screen	cg-reverb 3800 5000 add: tracker-screen	tracker-unclog-midi 3800 4200 add: TRACKER-screen	tracker-shortest-note 3800 4500 add: TRACKER-screen	cg-pedal-function 3600 3500 add: tracker-screen;: INIT.TRACKER-SCREEN	build.tracker.controls;: TERM.TRACKER-SCREEN	freeall: tracker-screen	free: tracker-screen;: TRAIN.PLAYER	'c midi.noteon mp-on-vector !	'c midi.noteoff mp-off-vector !	midi.parse.loop;if.forgotten term.tracker-screen