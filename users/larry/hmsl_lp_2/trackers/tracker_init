\ tracker init fileanew task-tracker_init: TRACKER.INIT	midi.clear	midi.parser.on	0 t-section ! \ sets section for beginning of piece	0 curr-tracker !	init.tracker.objects tab ." objects initialized " cr	init.tracker.record tab ." record grid initialized " cr	init.divider-job tab ." divider-job initialized " cr	init.tracker-job tab ." job initialized " cr	init.flammer tab ." flammer initialized " cr	init.blaster tab ." blaster initialized " cr	init.fbo1 tab ." fbo1 initialized " cr	init.drum-follow	init.tracker-screen tab ." screen initialized " cr	init.reverb 	cr cr cr ;: TRACKER.TERM	midi.killall	midi.alloff	term.tracker.objects	term.tracker-job	term.flammer	term.tracker.record	term.tracker-screen;tracker.init	if.forgotten tracker.term