\ trackers_reverbanew task-tracker_reverb: INIT.REVERB	2 midi.channel!	15 midi.preset;: CG-REVERB.FUNCTION { val part -- }	part CASE		0 of			 2 midi.channel!			val 1+ midi.preset		endof	ENDCASE;: BUILD.CG-REVERB	1 1 new: cg-reverb	250 300 put.wh: cg-reverb	9 put.text.size: cg-reverb	'c cg-reverb.function put.down.function: cg-reverb	" Reverb " put.title: cg-reverb	1 0 put.min: cg-reverb	127 0 put.max: cg-reverb	just_reverb 0 put.value: cg-reverb	stuff{ " preset " }stuff.text: cg-reverb;