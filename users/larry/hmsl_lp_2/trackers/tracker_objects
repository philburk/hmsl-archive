\ trackers objects\ 1/16/95\ lpanew task-tracker_objects: INIT.TRACKER-NOTE-ARRAY	60 1 new: tracker-note-array	stuff{ 		58 57 56 55 54 53 51 50 49 48 47 45 43 42 41 39 38 36		62 63 64 67 68 69 70 71 72 75 82 84 85 86 87 88 89 90 91 93 	}stuff: tracker-note-array;\ this uses the offset of the instrument as the note to be played., and the actual\ recorded velocity: TRACKER-INTERPRETER.VEL {  el shape inst -- }	drum_channel midi-channel !	inst get.offset: [] 	el 2 shape ed.at: []	midi.noteon;\ this uses the offset of the instrument as the note to be played, but the\ pitch of the trumpet as the velocity: TRACKER-INTERPRETER.PIT {  el shape inst -- }	drum_channel midi-channel !	inst get.offset: [] 	el 1 shape ed.at: []	47 - 3 *	midi.noteon;: TRACKER-REPEAT-FUNCTION { player -- }	tracker-random-repeat? @ true =	IF		rtc.rate@ choose		player put.repeat.delay: []	THEN; 	: INIT.TRACKER-PLAYERS  { | curr-player instr  -- }	100 0 DO		instantiate ob.player -> curr-player		curr-player add: tracker-player-list\ first setup instrument		instantiate ob.midi.instrument 		-> instr 		many: tracker-note-array choose		at: tracker-note-array		instr put.offset: []		drum_channel instr put.channel: []		\ this is the default, it can switch...		'c tracker-interpreter.pit instr put.on.function: []\ noe set up player		instr curr-player put.instrument: []		1 curr-player new: []		curr-player use.absolute.time: []		'c tracker-repeat-function curr-player put.repeat.function: []		100 curr-player put.repeat: []		3 curr-player put.on.dim: []	LOOP;: INIT.TRACKER.OBJECTS 	100 new: tracker-list	100 new: tracker-player-list	init.tracker-note-array	init.tracker-players;: TEST.TRACKER-NOTE-ARRAY	many: tracker-note-array 0	DO		i at: tracker-note-array 60 midi.noteon		i . key drop	LOOP;: TERM.TRACKER.OBJECTS	freeall: tracker-list	free: tracker-note-array	freeall: tracker-player-list;if.forgotten term.tracker.objects