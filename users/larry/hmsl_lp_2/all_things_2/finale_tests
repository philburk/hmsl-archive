\ test for finale\ play out as midi\ lp 10/10/98ob.player foo-playerob.shape foo-shape6 k: #_ticks4 #_ticks * ticks/beat !		: INIT.FOO-SHAPE	50 3 new: foo-shape	50 0 DO		2 60 96 add: foo-shape	LOOP;: foo-PLAYER.DURATION.FUNCTION { el sh -- }	el 0 sh ed.at: []	#_ticks *;	: INIT.PLAYER	\ set up timing for player	init.foo-shape	1 new: foo-player	foo-shape add: foo-player	ins-midi-1 put.instrument: foo-player	'c foo-player.duration.function put.dur.function: foo-player	0 put.offset: ins-midi-1;: TP	foo-player hmsl.play;: foo.CAPTURE	" all_things:foomidi" $midifile0{	tp	}midifile0;: GO	init.player	foo.capture;