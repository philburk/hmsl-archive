\ 51 live inititalization and printinganew task-51_live_init6 value month11 value date: INIT.PRESETS	perc 1+ midi.channel! perc 0 ed.at: 51-presets midi.preset	arp 1+ midi.channel! arp 0 ed.at: 51-presets midi.preset	chord 1+ midi.channel! chord 0 ed.at: 51-presets midi.preset	accent 1+ midi.channel! accent 0 ed.at: 51-presets midi.preset	mel-2 1+ midi.channel! mel-2 0 ed.at: 51-presets midi.preset	mel-1 1+ midi.channel! mel-1 0 ed.at: 51-presets midi.preset;	: INIT.51.LIVE	init.51-live-objects ." data initialized " 	init.51-constants ." constants initialized " 	init.arps ." arps initialized " cr	init.chords ." chords initialized " cr	init.accents ." accents initialized " cr	init.perc ." percussion initialized " cr	init.melodies ." melodies initialized " cr	init.51-live-coll ." collection initialized " cr	51.midi.input.init ." MIDI input initialized " cr	init.presets ." MIDI Presets initialized " cr	init.51-screen ." screen initialized " 	 cr tab ." Date is: " month . ." / " date . cr 	print.channels	cr cr cr tab ." Type GO to start piece " cr cr cr 12 set.text.size	24 set.text.size	run.faster	hmsl-graphics on	hmsl.start;\ some aliases...: 51.INIT	init.51.live;: INIT.51	init.51.live;: DO.51.LIVE	51-live-coll hmsl.play;: TERM.51.LIVE	term.51-live-objects	term.51-constants	term.arps	term.chords	term.accents	term.perc	term.melodies	term.51-live-coll	term.51.midi.input	term.51-screen;: GO	do.51.live;: ALLEZ	go;: NEWGO	start: 51-live-coll;\ go16 set.text.size cr cr cr tab ." Type 51.INIT to initialize piece " cr cr cr 12 set.text.sizecr cr cr tab ." and then Type ALLEZ to start piece " cr cr cr 12 set.text.size