\ play the harmoniesanew task-51_playersob.player 51-player: 51-PLAYER.DUR.FUNC { el# shape -- dur }	el# shape many: [] 1- = 	IF		d-dur 3 *	ELSE		d-dur	THEN;	: INIT.51-PLAYER { | curr-list -- }	#-arps @ 5 + new: 51-player	tonic-shape add: 51-player	51 0 DO		i at: arp-list		-> curr-list		curr-list many: [] 0 DO			i curr-list at: []			add: 51-player		LOOP	LOOP	dom-shape add: 51-player	ins-midi-1 put.instrument: 51-player	0 put.offset: ins-midi-1	'c 51-player.dur.func put.dur.function: 51-player;init.51-player: TERM.51-PLAYER	free: 51-player;if.forgotten term.51-player	