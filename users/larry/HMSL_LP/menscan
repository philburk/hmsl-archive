\ Mensuration Canon\ by Phil BurkANEW TASK-MENSURATE: WRAN  ( N -- )	60	swap 0	DO		dup note		3 choose+/- +	LOOP	drop;: MELODY  ( -- , basic melody )	1/4	C3 G B F	4 WRAN	C4 G A E	4 WRAN	;variable VT1: 1MC  ( channel -- , 1 part of mensuration )	midi.channel!	vtime@ vt1 !              \ get time melody started	>newline vt1 @ dup . 	melody	vtime@	dup .	vt1 @ - 3 8 */	vt1 @ +	dup . cr	vtime!  \ calculate time halfway through melody	tpw@ 5 8 */ tpw!;: 4MC 1 1mc 2 1mc 3 1mc 4 1mc ;