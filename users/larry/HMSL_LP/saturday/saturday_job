\ saturday job file\ 8/22/92anew task-saturday_jobOB.JOB SATURDAY-JOB: SATURDAY-JOB.FUNC-1 { job | job-dur -- }	300 30 wchoose -> job-dur	job-dur job put.duration: []\	job-dur . tab;: SATURDAY-JOB.FUNC-2 { job -- }	2 midi.channel!	100 choose 75 < 	IF		36 choose midi.preset	THEN	midi.lastoff	100 60 wchoose 96 30 wchoose \	2dup . . cr	midi.noteon	;: BUILD.SATURDAY-JOB	'c saturday-job.func-1 add: saturday-job	'c saturday-job.func-2 add: saturday-job;: INIT.SATURDAY-JOB	2 new: saturday-job	build.saturday-job\	print: saturday-job;: TERM.SATURDAY-JOB	free: saturday-job;