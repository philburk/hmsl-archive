: scan.a.circs { | slot# -- }	dsp_max_circuits 0	DO			." slot#: " i . space		i 56k.slot.code@ ." code: " . space		i 56k.slot.off?  ." off?: " . tab 		i 56k-MultiAudio  + dsp.p@ 		." contents: " . cr	LOOP	cr cr;: scan.c.circs { | slot# -- }	dsp_max_circuits 0	DO			." slot#: " i . space		i 56k.slot.code@ ." code: " . space		i 56k.slot.off?  ." off?: " . tab 		i 56k-MultiControl  + dsp.p@ 		." contents: " . cr	LOOP	cr cr;: scanem	cr	." AUDIO: " cr	scan.a.circs	." CONTROL: " cr	scan.c.circs ;: stuff.slots	9 0 DO		165 i 56k-multiaudio + dsp.p!		165 i 56k-multicontrol + dsp.p!	loop;