anew task-init_yitgadal	: INIT.YITGADAL	#_strings #_harmonics  new: violin1-array	#_strings #_harmonics  new:  violin2-array	#_strings #_harmonics  new: viola-array	#_strings #_harmonics  new: cello-array	\		#_strings #_harmonics  new: violin1-pitches	#_strings #_harmonics  new:  violin2-pitches	#_strings #_harmonics  new: viola-pitches	#_strings #_harmonics  new: cello-pitches	\	piece_length #_instruments 1+ new: piece-chords \ how many chords in the piece	#_instruments  1+ new: test-chord \ extra dimension for gs...	stuff{ 0 0 0 0 0 }stuff: test-chord		\	gs_chord_max new: gs-hit-array	gs_chord_max 0 DO 0 i to: gs-hit-array LOOP		#_instruments new: string-arrays	#_instruments new: pitch-arrays	\ single chord recording	#_instruments new: chord-string	#_instruments new: chord-harm	#_instruments 0 DO		0 i to: chord-string		0 i to: chord-harm	LOOP	\	stuff{ cello-array viola-array violin2-array violin1-array }stuff: string-arrays	stuff{ cello-pitches viola-pitches violin2-pitches violin1-pitches }stuff: pitch-arrays	#_strings dup * new: string-fundamentals	stuff.fundamental-arrays	stuff.string-arrays\			piece_length #_instruments new: chord-string-list	piece_length #_instruments new: chord-harm-list	piece_length 0 DO		#_instruments 0 DO			0 dup dup		LOOP		add: chord-string-list		add: chord-harm-list		0 add: piece-chords \ final entry is for gs of chord	LOOP	init.section-array;		: TERM.YITGADAL	free: violin1-array	free: cello-array	free: violin1-array	free: viola-array	free:  string-fundamentals	free: string-arrays	free: gs-hit-array	free: section-array;init.yitgadalif.forgotten term.yitgadal