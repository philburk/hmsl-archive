OS_PUSH		;	( n -- , push onto object stack )				MOVE.L		FV_OSSTACKPTR,A0  				MOVE.L		D7,-(A0)			; save old top				MOVE.L		(A6)+,D7  				MOVE.L		A0,FV_OSSTACKPTR  				RTSOS_DROP 	;	( -- , drop top of object stack )				MOVE.L		FV_OSSTACKPTR,A0				MOVE.L		(A0)+,D7  				MOVE.L		A0,FV_OSSTACKPTR	FWORDEND	OS_DROPOS_COPY		;	( -- N , make copy of top of object stack )				MOVE.L		D7,A0				MOVE.L		A0,-(A6)	FWORDEND	OS_COPYOS_PLUS		;	( M -- N+M , add top of object stack to data stack )				POP			D0				ADD.L		D7,D0				PUSH		D0	FWORDEND	OS_PLUSOS_PLUS_PUSH  ; ( N -- , Add to OS TOP and push onto object stack )				POP			D0				MOVE.L		FV_OSSTACKPTR,A0  				MOVE.L		D7,-(A0)			; save old top  				MOVE.L		A0,FV_OSSTACKPTR				ADD.L		D0,D7				RTS