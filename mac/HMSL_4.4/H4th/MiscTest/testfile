: SYSDUMP CR    SPAN @ . CR	STATE @ . CR	TIB #TIB @ TYPE CR	FSOURCE @ . CR;: CONSTANT    CREATE ,	DOES> @;: IMMEDIATE    latest nfa->ffa dup c@ 1 OR	swap c!;: VARIABLE    CREATE 0 ,;0 constant FALSE0 1 - constant TRUE: ON  true swap ! ;: OFF false swap ! ;2 3 + .  cr cr cr .S777 8788 + .  cr10 9 - .  cr .S: FOO dup + ; .S: GIMME BL WORD COUNT TYPE ; .S5 1 + .  cr .S