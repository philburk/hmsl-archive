\ Test TurnkeyANEW TASK-Test_Turnkey: APPL.INIT  ( -- )    ." Initializing Application!!!" cr;: APPL.MAIN  ( -- , main entry point for application )    100 0	DO i . cr?	LOOP	." Hit any key to continue!" key drop cr;: APPL.TERM  ( -- )    ." Terminating Application!" cr	." Hit any key to continue!" key drop cr;: TEST  ( -- )	'c appl.init	'c appl.main	'c appl.term  TurnKey;