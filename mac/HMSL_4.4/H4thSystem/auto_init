\ Execute CLI command line\ Define automatic actions for BYE\\ Your AUTO.xxxx words should call the previous AUTO.xxxx, eg.\\ : AUTO.INIT  auto.init init.my.stuff ;\    then in some other file\ : AUTO.INIT  auto.init init.other.stuff ;\\ *********************************************************************\ * HMSL Forth System                                                 *\ * Author: Phil Burk                                                 *\ * Copyright 1989 Phil Burk , All Rights Reserved                    *\ *********************************************************************ANEW TASK-AUTO_INIT: AUTO.TERM  ( -- , should be called by all others )    >newline ." AUTO.TERM" cr;: DO.AUTO.TERM  ( -- , search for auto.term and execute it )    " AUTO.TERM" find    IF execute    ELSE drop    THEN;: AUTO.INIT  ( -- )	." Started AUTO.INIT" cr;: DO.AUTO.INIT ( -- , search for auto.init and execute it )    " AUTO.INIT" find    IF		execute		." Finished AUTO.INIT" cr    ELSE drop    THEN;'c DO.AUTO.INIT is UserInit'c DO.AUTO.TERM is UserTerm: SAVE-FORTH  ( -- terminate as much as possible before saving )	do.auto.term	save-forth	do.auto.init;