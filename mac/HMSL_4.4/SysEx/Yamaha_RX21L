\ Support for Yamaha-RX21L\\ We currently only support the note mapping for the drum sounds.include? notate{ ht:notateANEW TASK-YAMAHA_RX21L\ Write words for use with score entry system.: CONGA.LO    ( -- )  64 note ;: CONGA.HI    ( -- )  65 note ;: CONGA.MUTE  ( -- )  66 note ;: BONGO.LO    ( -- )  67 note ;: BONGO.HI    ( -- )  68 note ;: TIMBALE.LO  ( -- )  69 note ;: TIMBALE.HI  ( -- )  70 note ;: TAMBOURINE  ( -- )  71 note ;: COWBELL     ( -- )  72 note ;: CLAVES      ( -- )  73 note ;: AGOGO.LO    ( -- )  74 note ;: AGOGO.HI    ( -- )  75 note ;: CUICA.LO    ( -- )  76 note ;: CUICA.HI    ( -- )  77 note ;: WHISTLE1    ( -- )  78 note ;: WHISTLE2    ( -- )  79 note ;\ Change FALSE to TRUE to load these rhythms.FALSE .IF  ( disable compilation ): ALL.DRUMS ( -- , play all drums using score entry system )     playnow 1/4 conga.hi conga.lo conga.mute     bongo.lo  bongo.hi  timbale.lo  timbale.hi     tambourine  cowbell  claves  agogo.lo  agogo.hi     cuica.lo  cuica.hi  whistle1  whistle2;: AGOGO.RIFF1 ( -- , simple riff )    1/8 agogo.hi agogo.hi 1/4 agogo.lo    1/16 agogo.hi agogo.hi rest agogo.hi 1/4 agogo.lo;: LATIN1  ( -- , african rhythm score  )    par{  1/2 conga.lo conga.lo conga.lo conga.lo    }par{ 1/4 rest 1/2 conga.hi conga.hi     }par{ 1/2 rest 1/32 4 0 DO timbale.hi LOOP          1/8 3 0 DO timbale.hi LOOP    }par{ 1/2 rest rest 1/8 cuica.hi cuica.hi cuica.lo rest    }par{ agogo.riff1 agogo.riff1    }par;: AFRICAN.RHYTHM  ( -- , the original pattern 19 on the RX21L )    par{  q3 conga.lo conga.lo conga.lo             conga.hi conga.lo rest             conga.lo conga.lo conga.hi             conga.lo rest conga.lo    }par{ q3 timbale.hi timbale.lo timbale.lo             timbale.hi timbale.lo rest          eh3 timbale.hi timbale.hi q3 timbale.lo timbale.lo          q3 timbale.hi timbale.lo rest    }par{ q3 rest eh3 agogo.lo agogo.hi q3 agogo.lo             agogo.hi agogo.lo agogo.lo             agogo.hi agogo.lo agogo.lo          eh3 agogo.hi agogo.lo q3 agogo.hi agogo.lo    }par{ q3 cuica.lo cuica.lo rest  cuica.hi cuica.lo rest             cuica.hi cuica.lo rest             cuica.hi cuica.lo cuica.lo    }par;: SAMBA.CONGA ( -- , conga part of samba )    1/8 rest conga.hi conga.hi conga.lo    conga.lo rest conga.hi conga.hi    rest conga.hi conga.mute conga.lo    conga.lo rest conga.hi conga.hi;: SAMBA.BONGO ( -- , bongo part of samba )    1/8 bongo.lo rest rest bongo.hi    bongo.hi rest bongo.lo rest    1/8 bongo.lo rest rest bongo.hi    bongo.hi rest bongo.lo rest    ;: SAMBA.CLAVES ( -- , claves part of samba )    1/8 rest rest claves claves    claves claves rest claves    claves claves rest claves    rest claves claves rest;: SAMBA.RHYTHM  ( -- , PATTERN08 on RX21L )    par{ samba.conga }par{ samba.bongo    }par{ 1/8 16 0 DO tambourine LOOP    }par{ 1/2 cowbell cowbell cowbell 1/4 cowbell cowbell    }par{ samba.claves    }par{ 1/8 8 0 DO rest cuica.hi LOOP    }par;.THEN